import java.util.*;
class Main{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        String s = sc.nextLine();
        String d = "";
        for(int i=s.length()-1 ;i>-1;i--){
            d=d+s.charAt(i);
    }
        if(d.equals(s)){
            System.out.println("yes");
        }
        else{
            System.out.println("no");
        }
    }
}
