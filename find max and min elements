import java.util.ArrayList;
import java.util.Collections;
import java.util.Scanner;
class collection0 {
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int n=sc.nextInt();
        ArrayList<Integer> a = new ArrayList<>();
        for(int i=0;i<=n;i++){
            int c=sc.nextInt();
            a.add(c);
        }
        int minlist=Collections.min(a);
        int maxlist=Collections.max(a);
        System.out.println("Minimum element in list: "+minlist);
        System.out.println("Maximum element in list: "+maxlist);
    }
}
