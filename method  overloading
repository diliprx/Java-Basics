import java.util.*;

class Adder{
 static int add(int a,int b){
     return a+b;
 }   
 static int add(int a,int b, int c){
     return a+b+c;
 }
}
public class Main{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int m = sc.nextInt();
        int p = sc.nextInt();
        System.out.println(Adder.add(n, m));
        System.out.println(Adder.add(n,m, p));
    }
}
